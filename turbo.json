{
  "$schema": "https://turbo.build/schema.json",
  "globalDotEnv": [".env"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "db:migrate:dev": {},
    "db:migrate:deploy": {},
    "db:push": {},
    "db:seed": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "watch": {
      "dependsOn": ["^watch"],
      "persistent": true
    },
    "generate": {
      "dependsOn": ["^generate"]
    },
    "lint": {},
    "clean": {
      "cache": false
    },
    "clean:all": {
      "cache": false
    }
  }
}
