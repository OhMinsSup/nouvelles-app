{
  "$schema": "https://turbo.build/schema.json",
  "globalDotEnv": [".env"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "server:build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "server:start": {
      "cache": false,
      "persistent": true
    },
    "db:migrate:dev": {
      "cache": false
    },
    "db:migrate:deploy": {},
    "db:push": {},
    "db:studio": {
      "cache": false
    },
    "env:build": {},
    "env:watch": {
      "cache": false
    },
    "watch": {
      "dependsOn": ["^watch"],
      "persistent": true
    },
    "generate": {
      "dependsOn": ["^generate"]
    },
    "lint": {},
    "clean": {
      "cache": false
    }
  }
}
