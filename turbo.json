{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env"],
  "globalEnv": [
    "NODE_ENV",
    "FLY_REGION",
    "PRIMARY_REGION",
    "CI",
    "PORT"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "db:migrate:deploy": {
      "outputs": []
    },
    "db:migrate:dev": {
      "outputs": []
    },
    "db:push": {
      "outputs": []
    },
    "db:seed": {
      "outputs": [],
      "cache": false
    },
    "watch": {
      "dependsOn": ["^watch"],
      "persistent": true
    },
    "generate": {
      "dependsOn": ["^generate"]
    },
    "lint": {},
    "clean": {
      "cache": false
    },
    "docker:build": {
      "outputs": [],
      "cache": false
    }
  }
}
