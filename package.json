{
  "name": "nouvelles",
  "version": "0.0.1-beta.0",
  "private": true,
  "description": "nouvelles",
  "keywords": [
    "nextjs",
    "vercel",
    "supabase"
  ],
  "license": "ISC",
  "author": "",
  "type": "module",
  "scripts": {
    "build": "dotenv -- turbo run build",
    "clean": "turbo clean",
    "db:migrate:deploy": "dotenv -- turbo run db:migrate:deploy",
    "db:migrate:dev": "dotenv -- turbo run db:migrate:dev",
    "db:push": "dotenv -- turbo run db:push",
    "dev": "dotenv -- turbo run dev --concurrency 255",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "server:build": "dotenv -- turbo run server:build",
    "server:start": "dotenv -- turbo run server:start --concurrency 13",
    "watch": "turbo run watch --concurrency 50%"
  },
  "devDependencies": {
    "@nouvelles/lintconfig": "workspace:*",
    "@nouvelles/tsconfig": "workspace:*",
    "cspell": "^8.1.3",
    "dotenv-cli": "^7.3.0 ",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "syncpack": "^11.2.1",
    "turbo": "^1.11.2",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.12.1",
  "engines": {
    "bun": "1.x",
    "node": "18.x || 20.x"
  },
  "prisma": {
    "schema": "packages/database/prisma/schema.prisma"
  }
}
